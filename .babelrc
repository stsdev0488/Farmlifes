{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/assets",
          "_actions": "./src/actions",
          "_reducers": "./src/reducers",
          "_utils": "./src/utils",
          "_config": "./src/config",
          "_components": "./src/components",
          "_navigations": "./src/navigations",
          "_services": "./src/services",
          "_containers": "./src/containers",
          "_screens": "./src/screens"
        }
      }
    ]
  ]
}
