{
  "name": "farmlifes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "npm-run-all --parallel lint:*",
    "eslint:fix": "eslint --fix . --ext .ts  ",
    "prettier": "prettier './src/**/*.{ts,tsx}' --single-quote --no-semi --write",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc --noEmit",
    "test": "jest",
    "adb": "adb logcat",
    "release": "react-native run-android --variant=release",
    "android:release-bundle": "cd android &&  ./gradlew bundleRelease",
    "release-app": "cd android && ./gradlew assembleRelease",
    "clear": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start -- --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/netinfo": "^4.6.0",
    "@react-native-firebase/admob": "^6.2.0",
    "@react-native-firebase/analytics": "^6.2.0",
    "@react-native-firebase/app": "^6.2.0",
    "@react-native-firebase/crashlytics": "^6.2.0",
    "axios": "^0.19.0",
    "babel-preset-react-native": "^4.0.1",
    "javascript-time-ago": "^2.0.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "native-base": "^2.13.8",
    "prop-types": "^15.7.2",
    "react": "16.8.1",
    "react-native": "0.61.2",
    "react-native-animatable": "^1.3.3",
    "react-native-autogrow-textinput": "^5.3.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-flash-message": "^0.1.15",
    "react-native-geolocation": "^1.0.0",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-gifted-chat": "^0.12.0-beta-0",
    "react-native-google-places-autocomplete": "^1.4.0",
    "react-native-i18n": "git+https://github.com/lemankk/react-native-i18n.git#e870017bd93c9b2e3762ae83a2db3de8c4257146",
    "react-native-image-crop-picker": "^0.26.1",
    "react-native-image-picker": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-maps": "0.26.1",
    "react-native-modal": "^11.4.0",
    "react-native-onesignal": "^3.5.0",
    "react-native-parsed-text": "0.0.21",
    "react-native-picker-select": "^6.3.3",
    "react-native-reanimated": "^1.3.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.0.2",
    "react-native-video-controls": "^2.4.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.1",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-preset-expo": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/runtime": "^7.5.0",
    "@react-native-community/eslint-config": "^0.0.3",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.6",
    "@types/react-native": "^0.60.19",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.1.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.51.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "16.8.1",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
